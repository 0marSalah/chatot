{
    "compilerOptions": {
        "outDir": "./dist",
        "rootDir": "./src",
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    // alias
    "baseUrl": "./",
    "paths": {
        "@/*": ["src/*"]
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
